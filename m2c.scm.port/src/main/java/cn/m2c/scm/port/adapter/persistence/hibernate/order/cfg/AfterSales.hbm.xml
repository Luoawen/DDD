<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.scm.domain.model.order.fsales.AfterSales" table="t_order_after_sales" lazy="true">
		<id name="id" type="long" column="id" length="11">
			<generator class="native" />
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="int" />
		
		<property name="fsalesId" column="fsales_id" type="string" />
		<property name="orderId" column="order_id" type="string" />
		<property name="applyReason" column="apply_reason" type="string" />
		<property name="fsalesStatus" column="fsales_status" type="int" />
		<property name="applyTime" column="apply_time" type="long" />
		<property name="barterTime" column="barter_time" type="long" />
		<property name="auditTime" column="audit_time" type="long" />
		<property name="finishedTime" column="finished_time" type="long" />
		<property name="refundAmount" column="refund_amount" type="long" />
		<property name="unauditReason" column="unaudit_reason" type="string" />
		<property name="refbaredReason" column="refbared_reason" type="string" />
		<property name="handleStatus" column="handle_status" type="int" />

	</class>
</hibernate-mapping>
