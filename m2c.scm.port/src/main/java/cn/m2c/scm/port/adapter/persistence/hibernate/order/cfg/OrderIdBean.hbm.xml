<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
    <class name="cn.m2c.scm.domain.model.order.OrderIdBean" table="t_scm_order_order_no" lazy="true"
           dynamic-insert="true" dynamic-update="true">

        <id name="id" column="id" type="long" unsaved-value="-1">
            <generator class="native"/>
        </id>
        <property name="time" column="time_str" type="string"/>
        <property name="suffix" column="suffix" type="string"/>
        <property name="orderNo" column="orderNo" type="string"/>
    </class>
</hibernate-mapping>
