<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.scm.domain.model.goods.goods.Goods" table="t_goods_goods" lazy="true" 
	optimistic-lock="version" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="id" type="long" unsaved-value="-1">
			<generator class="native" />
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="int" />
		<property name="createdDate" generated="insert" not-null="true">
			<column name="created_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		<property name="lastUpdatedDate" generated="always" not-null="true">
			<column name="last_updated_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		
		
		<property name="goodsId" column="goods_id" type="string" />
		<property name="dealerId" column="dealer_id" type="string" />
		<property name="dealerName" column="dealer_name" type="string" />
		<property name="cooperationMode" column="cooperation_mode" type="int" />
		<property name="firstClassify" column="first_classify" type="string" />
		<property name="secondClassify" column="second_classify" type="string" />
		<property name="goodsNo" column="goods_no" type="string" />
		<property name="barNo" column="bar_no" type="string" />
		<property name="goodsName" column="goods_name" type="string" />
		<property name="subtitle" column="subtitle" type="string" />
		<property name="salePrice" column="sale_price" type="long" />
		<property name="marketPrice" column="market_price" type="long" />
		<property name="brandId" column="brand_id" type="string" />
		<property name="propertyId" column="property_id" type="string" />
		<property name="transportFeeId" column="transport_fee_id" type="string" />
		<property name="guarantee" column="guarantee" type="string" />
		<property name="advertisementPic" column="advertisement_pic" type="string" />
		<property name="gallery" column="gallery" type="string" />
		<property name="goodsDesc" column="goods_desc" type="string" />
		<property name="recognizedId" column="recognized_id" type="string" />
		<property name="recognizedUrl" column="recognized_url" type="string" />
		<property name="orderNum" column="order_num" type="int" />
		<property name="sellerNum" column="seller_num" type="int" />
		<property name="collectionNum" column="collection_num" type="int" />
		<property name="shareNum" column="share_num" type="int" />
		<property name="mediaId" column="media_id" type="string" />
		<property name="mediaName" column="media_name" type="string" />
		<property name="mresId" column="mres_id" type="string" />
		<property name="mresName" column="mres_name" type="string" />
		<property name="mresNum" column="mres_num" type="int" />
		<property name="goodsStatus" column="goods_status" type="int" />
		<property name="sellerId" column="seller_id" type="string" />
		<property name="sellerName" column="seller_name" type="string" />
		<property name="divideScale" column="divide_scale" type="string" />
		<property name="payGoodsNum" column="pay_goods_num" type="int" />
		<property name="changeGoodsNum" column="change_goods_num" type="int" />
		<property name="returnGoodsNum" column="return_goods_num" type="int" />
		<property name="upGoodsDate" column="up_goods_date" type="timestamp" />
		<property name="downGoodsDate" column="down_goods_date" type="timestamp" />
	</class>
</hibernate-mapping>
