<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.scm.domain.model.goods.templet.TransportFee" table="t_goods_transport_fee" lazy="true" 
	optimistic-lock="version" dynamic-insert="true" dynamic-update="true">
		
		<id name="id" column="id" type="long" unsaved-value="-1">
			<generator class="native" />
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="int" />
		<property name="createdDate" generated="insert" not-null="true">
			<column name="created_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		<property name="lastUpdatedDate" generated="always" not-null="true">
			<column name="last_updated_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		<property name="transportFeeId" column="transport_fee_id" type="string" />
		<property name="modelName" column="model_name" type="string" />
		<property name="fee" column="transport_fee" type="long" />
		<property name="dealerId" column="dealer_id" type="string" />
		<property name="transportFeeStatus" column="transport_fee_status" type="int" />
	</class>
</hibernate-mapping>
