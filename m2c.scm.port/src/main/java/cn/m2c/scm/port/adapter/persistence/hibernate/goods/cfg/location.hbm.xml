<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.scm.domain.model.goods.location.Location" table="t_goods_show_location" lazy="true" 
	optimistic-lock="version" dynamic-insert="true" dynamic-update="true">
		
		<id name="id" column="id" type="long" unsaved-value="-1">
			<generator class="native" />
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="int" />
		<property name="createdDate" generated="insert" not-null="true">
			<column name="created_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		<property name="lastUpdatedDate" generated="always" not-null="true">
			<column name="last_updated_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>

		<property name="locationId" column="location_id" type="string" />
		<property name="locSelect" column="loc_select" type="int" />
		<property name="title" column="title" type="string" />
		<property name="isOnline" column="is_online" type="int" />
		<property name="effectiveTime" column="effective_time" type="string" />
		<property name="displayOrder" column="display_order" type="int" />
		<property name="imgUrl" column="img_url" type="string" />
		<property name="locType" column="loc_type" type="int" />
		<property name="redirectUrl" column="redirect_url" type="string" />
		<property name="goodsId" column="goods_id" type="string" />
		<property name="goodsName" column="goods_name" type="string" />
		<property name="locStatus" column="loc_status" type="int" />
		<property name="upDate" column="up_date" type="timestamp" />
		<property name="goodsPrice" column="goods_price" type="long" />
	</class>
</hibernate-mapping>
