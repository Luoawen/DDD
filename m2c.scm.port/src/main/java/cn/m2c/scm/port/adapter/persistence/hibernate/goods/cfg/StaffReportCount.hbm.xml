<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.goods.domain.seller.StaffReportCount" table="t_goods_report_count" lazy="true" 
	optimistic-lock="version" dynamic-insert="true" dynamic-update="true">
		
	<id name="reportId" column="report_id" type="integer" length="11">
			<generator class="native"></generator>
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="integer" />
		<property name="isValid" generated="insert">
			<column name="is_valid" sql-type="integer" default="1" not-null="true"></column>
		</property>
		<property name="createdDate" generated="insert" not-null="true">
			<column name="created_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>
		<property name="lastUpdatedDate" generated="always" not-null="true">
			<column name="last_updated_date" sql-type="timestamp" default="CURRENT_TIMESTAMP"></column>
		</property>

		<property name="staffId" column="staff_id" type="string" />
		<property name="objType" column="obj_type" type="integer" />
		<property name="countNum" column="count_num" type="integer" />
		<property name="reportDate" column="report_date" type="timestamp" />
	</class>
</hibernate-mapping>
