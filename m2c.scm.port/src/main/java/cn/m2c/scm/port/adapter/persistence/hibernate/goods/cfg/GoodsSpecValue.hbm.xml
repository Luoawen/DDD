<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
    <class name="cn.m2c.scm.domain.model.goods.GoodsSpecValue" table="t_scm_goods_spec_value" lazy="true"
           optimistic-lock="version" dynamic-insert="true" dynamic-update="true">

        <id name="id" column="id" type="long" unsaved-value="-1">
            <generator class="native"/>
        </id>
        <version name="concurrencyVersion" column="concurrency_version" type="int"/>
        <property name="specId" column="spec_value_id" type="string"/>
        <property name="dealerId" column="dealer_id" type="string"/>
        <property name="specValue" column="spec_value" type="int"/>
    </class>
</hibernate-mapping>
