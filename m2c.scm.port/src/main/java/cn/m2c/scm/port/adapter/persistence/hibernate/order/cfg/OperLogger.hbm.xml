<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
	<class name="cn.m2c.order.domain.logger.OperLogger" table="t_order_oper_logger" lazy="true">
		<id name="id" type="long" column="id" length="11">
			<generator class="native" />
		</id>
		<version name="concurrencyVersion" column="concurrency_version" type="int" />
		
		<property name="loggerId" column="logger_id" type="string" />
		<property name="businessId" column="business_id" type="string" />
		<property name="businessType" column="business_type" type="int" />
		<property name="operName" column="oper_name" type="string" />
		<property name="operDes" column="oper_des" type="string" />
		<property name="operResult" column="oper_result" type="string" />
		<property name="operTime" column="oper_time" type="long" />
		<property name="operUserId" column="oper_user_id" type="string" />
		<property name="operUserName" column="oper_user_name" type="string" />

	</class>
</hibernate-mapping>
